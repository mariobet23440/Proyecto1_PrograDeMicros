                                 // DECREMENTAR CONTADOR --------------------------------------------
                                 DECREMENTAR_PC:
                                 	// CambiarMinutos (S1)
0006a9 fd80                      	SBRC	STATE, S1B	
0006aa 940c 06bb                 	JMP		DECREMENTAR_MINUTOS
                                 
                                 	// CambiarHoras (S2)
0006ac fd81                      	SBRC	STATE, S2B
0006ad 940c 06c3                 	JMP		DECREMENTAR_HORAS
                                 		
                                 	// CambiarDias (S4)
0006af fd83                      	SBRC	STATE, S4B
0006b0 940c 06cb                 	JMP		DECREMENTAR_DIAS
                                 
                                 	// CambiarMeses (S5)
0006b2 fd84                      	SBRC	STATE, S5B
0006b3 940c 06e7                 	JMP		DECREMENTAR_MESES
                                 
                                 	// AlarmaMinutos (S7)
0006b5 fd86                      	SBRC	STATE, S7B
0006b6 c038                      	RJMP	DECREMENTAR_ALARMA_MINUTOS
                                 
                                 	// AlarmaHoras (S8)
0006b7 fd87                      	SBRC	STATE, S8B
0006b8 c03e                      	RJMP	DECREMENTAR_ALARMA_HORAS
                                 
                                 	// Si no es ni fu ni fa salir
0006b9 940c 0701                 	JMP		END_PC_ISR
                                 
                                 
                                 DECREMENTAR_MINUTOS:
                                 	// Comparar con cero
0006bb 3020                      	CPI		MINUTE_COUNT, 0
0006bc f019                      	BREQ	UNDERFLOW_MINUTOS
                                 	
                                 	// Si el contador de minutos es mayor a cero, restar uno
0006bd 952a                      	DEC		MINUTE_COUNT
0006be 940c 0701                 	JMP		END_PC_ISR
                                 
                                 // Si el contador de minutos da cero, cargar 59 y disminuir horas
                                 UNDERFLOW_MINUTOS:
0006c0 e32b                      	LDI		MINUTE_COUNT, 59
0006c1 940c 06c3                 	JMP		DECREMENTAR_HORAS
                                 
                                 // Si el nmero de horas no es cero, disminuir
                                 DECREMENTAR_HORAS:
                                 	// Comparar con cero
0006c3 3030                      	CPI		HOUR_COUNT, 0
0006c4 f019                      	BREQ	UNDERFLOW_HORAS
                                 	
                                 	// Si el contador es mayor a cero, restar uno
0006c5 953a                      	DEC		HOUR_COUNT
0006c6 940c 0701                 	JMP		END_PC_ISR
                                 	
                                 UNDERFLOW_HORAS:
0006c8 e137                      	LDI		HOUR_COUNT, 23
0006c9 940c 06cb                 	JMP		DECREMENTAR_DIAS
                                 
                                 DECREMENTAR_DIAS:
                                 	// Comparar con uno
0006cb 3041                      	CPI		DAY_COUNT, 1
0006cc f019                      	BREQ	UNDERFLOW_DIAS
                                 	
                                 	// Si el contador es mayor a cero, restar uno
0006cd 954a                      	DEC		DAY_COUNT
0006ce 940c 0701                 	JMP		END_PC_ISR
                                 
                                 // Esta funcin se parece mucho a COMPARACION_DIAS_MESES_INC pero al revs
                                 UNDERFLOW_DIAS:	
                                 	// Verificar si el mes es marzo (Underflow a febrero)
0006d0 3053                      	CPI		MONTH_COUNT, 3
0006d1 f079                      	BREQ	UNDERFLOW_MARZO
                                 
                                 	// Meses precedidos por meses de 31 das
0006d2 3051                          CPI		MONTH_COUNT, 1			; Enero (underflow a Diciembre)
0006d3 f079                          BREQ	UNDERFLOW_A_MES31
0006d4 3052                      	CPI		MONTH_COUNT, 2			; Febrero (underflow a Enero)
0006d5 f069                          BREQ	UNDERFLOW_A_MES31
0006d6 3054                      	CPI		MONTH_COUNT, 4			; Abril (underflow a Marzo)
0006d7 f059                          BREQ	UNDERFLOW_A_MES31
0006d8 3056                          CPI		MONTH_COUNT, 6			; Junio (underflow a Mayo)
0006d9 f049                          BREQ	UNDERFLOW_A_MES31
0006da 3058                      	CPI		MONTH_COUNT, 8			; Agosto (underflow a Junio)
0006db f039                          BREQ	UNDERFLOW_A_MES31
0006dc 3059                          CPI		MONTH_COUNT, 9			; Septiembre (underflow a Agosto)
0006dd f029                          BREQ	UNDERFLOW_A_MES31
0006de 305b                          CPI		MONTH_COUNT, 11			; Noviembre (underflow a Octubre)
0006df f019                          BREQ	UNDERFLOW_A_MES31
                                 
                                 	// Meses precedidos por meses de 30 das
0006e0 c004                      	RJMP	UNDERFLOW_A_MES30
                                 
                                 // Si el mes donde ocurre el overflow es marzo, cargar a 28 das
                                 UNDERFLOW_MARZO:
0006e1 e14c                          LDI		DAY_COUNT, 28
0006e2 c004                          RJMP	DECREMENTAR_MESES
                                 
                                 // Si el mes donde ocurre el overflow es enero, febrero, abril, junio, septiembre y noviembre
                                 UNDERFLOW_A_MES31:
0006e3 e14f                          LDI		DAY_COUNT, 31
0006e4 c002                      	RJMP	DECREMENTAR_MESES
                                 
                                 // Underflow a un mes de 30 das
                                 UNDERFLOW_A_MES30:
0006e5 e14e                          LDI		DAY_COUNT, 30
0006e6 c000                      	RJMP	DECREMENTAR_MESES
                                 
                                 // INCREMENTAR MES
                                 DECREMENTAR_MESES:
                                 	// Comparar con uno
0006e7 3051                      	CPI		MONTH_COUNT, 1
0006e8 f019                      	BREQ	UNDERFLOW_MESES
                                 
                                 	// Si el mes no es enero, decrementar contador
0006e9 955a                      	DEC		MONTH_COUNT
0006ea 940c 0701                 	JMP		END_PC_ISR
                                 
                                 UNDERFLOW_MESES:
0006ec e05c                      	LDI		MONTH_COUNT, 12
0006ed 940c 0701                 	JMP		END_PC_ISR
                                 
                                 DECREMENTAR_ALARMA_MINUTOS:
                                 	// Si el contador de minutos es 0, hacer un underflow
