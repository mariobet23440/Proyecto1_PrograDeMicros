00068d fd83                      	SBRC	STATE, S4B
00068e 940c 0727                 	JMP		COMPARACION_DIAS_MESES_INC
                                 
                                 	// CambiarMeses (S5)
000690 fd84                      	SBRC	STATE, S5B
000691 940c 073e                 	JMP		INCREMENTAR_MES
                                 
                                 	// AlarmaMinutos (S7)
000693 fd86                      	SBRC	STATE, S7B
000694 c004                      	RJMP	INCREMENTAR_ALARMA_MINUTOS
                                 
                                 	// AlarmaHoras (S8)
000695 fd87                      	SBRC	STATE, S8B
000696 c009                      	RJMP	INCREMENTAR_ALARMA_HORAS
                                 	
                                 	// Si no es ni fu ni fa salir
000697 940c 0701                 	JMP		END_PC_ISR
                                 
                                 INCREMENTAR_ALARMA_MINUTOS:
                                 	// Si el contador de minutos es de 59, hacer un overflow
000699 2d04                      	MOV		R16, ALARM_MINUTES
00069a 330b                      	CPI		R16, 59
00069b f019                      	BREQ	OVERFLOW_ALARMA_MINUTOS
                                 	
                                 	// Si no, incrementar contador y salir
00069c 9443                      	INC		ALARM_MINUTES
00069d 940c 0701                 	JMP		END_PC_ISR
                                 
                                 OVERFLOW_ALARMA_MINUTOS:
00069f 2444                      	CLR		ALARM_MINUTES
                                 	
                                 INCREMENTAR_ALARMA_HORAS:
                                 	// Si son las 23 horas hacer overflow
0006a0 2d05                      	MOV		R16, ALARM_HOUR
0006a1 3107                      	CPI		R16, 23
0006a2 f019                      	BREQ	OVERFLOW_ALARMA_HORAS
                                 
