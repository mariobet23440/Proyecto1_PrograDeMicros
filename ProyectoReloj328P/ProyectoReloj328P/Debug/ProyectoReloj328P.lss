                                 	// MostrarHoras (S0) -> MostrarFecha (S3)
00063c 3080                      	CPI		STATE, 0
00063d f419                      	BRNE	INTERMEDIATE_JUMP_PCINT_ISR
00063e e094                      	LDI		NEXT_STATE, S3
00063f 940c 0658                 	JMP		COPY_NEXT_STATE
                                 
                                 // Salto intermedio por limitaciones de BRNE
                                 INTERMEDIATE_JUMP_PCINT_ISR:
000641 940c 0658                 	JMP		COPY_NEXT_STATE
                                 	
                                 
                                 // Lgica de Siguiente estado si se presiona PB1
                                 NEXT_STATE_LOGIC_PB1:
                                 	// CambiarMinutos (S1) -> CambiarHoras (S2)
000643 fd80                      	SBRC	STATE, S1B
000644 e092                      	LDI		NEXT_STATE, S2
                                 
                                 	// CambiarHoras (S2) -> MostrarHora (S0)
000645 fd81                      	SBRC	STATE, S2B
000646 e090                      	LDI		NEXT_STATE, S0
                                 
