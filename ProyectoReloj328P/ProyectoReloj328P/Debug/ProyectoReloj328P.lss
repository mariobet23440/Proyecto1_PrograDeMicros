00062f e290                      	LDI		NEXT_STATE, S6
                                 
                                 	// CambiarDas (S4) -> MostrarFecha (S3)
000630 fd83                      	SBRC	STATE, S4B
000631 e094                      	LDI		NEXT_STATE, S3
                                 
                                 	// CambiarMeses (S5) -> MostrarFecha (S3)
000632 fd84                      	SBRC	STATE, S5B
000633 e094                      	LDI		NEXT_STATE, S3
                                 
                                 	// MostrarFecha (S3) -> ModoAlarma (S6)
000634 fd82                      	SBRC	STATE, S3B
000635 e290                      	LDI		NEXT_STATE, S6
                                 
                                 	// ModoAlarma (S6) -> MostrarHora (S0)
000636 fd85                      	SBRC	STATE, S6B
000637 e090                      	LDI		NEXT_STATE, S0
                                 
                                 	// AlarmaMinutos (S7) -> ModoAlarma (S6)
000638 fd86                      	SBRC	STATE, S7B
000639 e290                      	LDI		NEXT_STATE, S6
                                 
                                 	// AlarmaHoras (S8) -> ModoAlarma (S6)
00063a fd87                      	SBRC	STATE, S8B
