0006f3 940c 0701                 	JMP		END_PC_ISR
                                 
                                 UNDERFLOW_ALARMA_MINUTOS:
0006f5 e30b                      	LDI		R16, 59
0006f6 2e40                      	MOV		ALARM_MINUTES, R16
                                 	
