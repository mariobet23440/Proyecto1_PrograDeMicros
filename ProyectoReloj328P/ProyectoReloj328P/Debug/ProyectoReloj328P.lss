                                 // --------------------------------------------------------------------
                                 // | RUTINA NO DE INTERRUPCIN 3 - LOOKUP_TABLE (Verificado)		  |
                                 // --------------------------------------------------------------------
                                 // LOOKUP TABLE
                                 LOOKUP_TABLE:
                                 	// Guardar bit 7 de PORTD en el Bit T del SREG
0005ea b11b                      	IN		R17, PORTD
0005eb fb17                      	BST		R17, 7
                                 	
                                 	// Sacar dato de tabla
0005ec e0fa                      	LDI		ZH, HIGH(TABLA<<1)
0005ed e0e0                      	LDI		ZL, LOW(TABLA<<1)
                                 
                                 	// Obtener direccin de tabla (Duplicando OUT_PORTD)
0005ee 2f06                      	MOV		R16, OUT_PORTD
0005ef 0f06                      	ADD		R16, OUT_PORTD
                                 
                                 	// Incrementar puntero Z en 2*OUT_PORTD
0005f0 0fe0                      	ADD		ZL, R16
                                 
                                 	// Sacar el primer dgitooo
0005f1 9104                      	LPM		R16, Z
                                 	
                                 	// Si MUX_SIGNAL es 0x04 o 0x08 sacar el siguiente nmero
0005f2 fd71                      	SBRC	MUX_SIGNAL, 1
0005f3 95e3                      	INC		ZL
0005f4 fd73                      	SBRC	MUX_SIGNAL, 3
0005f5 95e3                      	INC		ZL
0005f6 9104                      	LPM		R16, Z
                                 
                                 	// Cargar bit de leds intermitentes
0005f7 f907                      	BLD		R16, 7
                                 
                                 	// Mostrar en PORTD
0005f8 b90b                      	OUT		PORTD, R16
                                 
0005f9 9508                      	RET
                                 
