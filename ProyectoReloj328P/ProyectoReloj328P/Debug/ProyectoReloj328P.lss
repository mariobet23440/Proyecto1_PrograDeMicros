                                 // INCREMENTAR CONTADOR --------------------------------------------
                                 // Incrementar contador con PC (Vamos a reciclar algunas subrutinas)
                                 INCREMENTAR_PC:	
                                 	// CambiarMinutos (S1)
000681 fd80                      	SBRC	STATE, S1B
000682 9523                      	INC		MINUTE_COUNT
000683 fd80                      	SBRC	STATE, S1B
000684 940c 071a                 	JMP		COMPARACION_MINUTOS_HORAS_INC
                                 
                                 	// CambiarHoras (S2)
000686 fd81                      	SBRC	STATE, S2B
