00064f fd86                      	SBRC	STATE, S7B
000650 e890                      	LDI		NEXT_STATE, S8
                                 
                                 	// AlarmaHoras (S8) -> ModoAlarma (S6)
000651 fd87                      	SBRC	STATE, S8B
000652 e290                      	LDI		NEXT_STATE, S6
                                 
                                 	// MostrarHoras (S0) -> CambiarMinutos (S2)
000653 3080                      	CPI		STATE, S0
000654 f419                      	BRNE	COPY_NEXT_STATE
000655 e091                      	LDI		NEXT_STATE, S1
000656 940c 0658                 	JMP		COPY_NEXT_STATE
                                 
